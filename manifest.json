{
    "name": "Ninja Crawler",
    "description" : "Base Level Extension",
    "version": "1.0",
    "permissions":[
      "tabs",
      "http://*.google.com/"
    ],
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "index.html",
      "default_icon": "ninja.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },    
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+F",
          "mac": "MacCtrl+Shift+F"
        },
        "description": "Opens hello.html"
      }
    },
    "content_security_policy":"script-src 'self' https://unpkg.com/npm-require; object-src 'self'"

}